<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hoon.gloriousfamily" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
        android:screenOrientation="portrait"
        android:largeHeap="true">
        <activity
            android:name=".MainActivity_"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ModeSelectActivity_" android:label="ModeSelectActivity" />
        <activity android:name=".ProfileActivity_" android:label="ProfileActivity" />
        <activity android:name=".ScenarioActivity_" android:label="ScenarioActivity" />
        <activity android:name=".HomeActivity_" android:label="HomeActivity" />
    </application>

</manifest>
